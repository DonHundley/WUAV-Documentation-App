<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="450.0" prefWidth="600.0" styleClass="NEwindow-background" stylesheets="@../../css/style.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.NEProjectController">
   <children>
      <Label fx:id="windowTitleLabel" layoutX="144.0" layoutY="38.0" styleClass="NEwindow-title" stylesheets="@../../css/style.css" text="Create New Project" />
      <Label layoutX="92.0" layoutY="135.0" styleClass="NEwindow-text" stylesheets="@../../css/style.css" text="*Project Name" />
      <TextField fx:id="projectName" layoutX="272.0" layoutY="133.0" prefHeight="25.0" prefWidth="213.0" />
      <Button fx:id="createOrEditProject" layoutX="92.0" layoutY="385.0" mnemonicParsing="false" onAction="#createOrEditProject" styleClass="round-btn" stylesheets="@../../css/style.css" text="Create" />
      <Button fx:id="cancelButton" layoutX="187.0" layoutY="386.0" mnemonicParsing="false" onAction="#cancel" styleClass="round-btn" stylesheets="@../../css/style.css" text="Cancel" />
      <Label layoutX="287.0" layoutY="390.0" styleClass="NEwindow-text" stylesheets="@../../css/style.css" text="required fields are marked with *" />
      <TableView fx:id="nEProjectTV" layoutX="40.0" layoutY="170.0" prefHeight="200.0" prefWidth="520.0">
        <columns>
          <TableColumn fx:id="customerName" prefWidth="180.0" text="Customer Name" />
          <TableColumn fx:id="customerEmail" prefWidth="143.0" text="Customer Email" />
            <TableColumn fx:id="customerAddress" prefWidth="196.0" text="Customer Address" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
