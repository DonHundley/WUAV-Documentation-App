<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="450.0" prefWidth="600.0" styleClass="NEwindow-background" stylesheets="@../../css/style.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.NECustomerController">
   <children>
      <Label fx:id="windowTitleLabel" layoutX="122.0" layoutY="22.0" styleClass="NEwindow-title" stylesheets="@../../css/style.css" text="Create New Customer" />
      <Label layoutX="119.0" layoutY="79.0" styleClass="NEwindow-text" stylesheets="@../../css/style.css" text="*Name" textFill="WHITE" />
      <TextField fx:id="customerName" layoutX="223.0" layoutY="77.0" prefHeight="25.0" prefWidth="260.0" />
      <Label layoutX="119.0" layoutY="129.0" styleClass="NEwindow-text" stylesheets="@../../css/style.css" text="*Email" textFill="WHITE" />
      <TextField fx:id="customerEmail" layoutX="223.0" layoutY="127.0" prefHeight="25.0" prefWidth="260.0" />
      <Label layoutX="118.0" layoutY="177.0" styleClass="NEwindow-text" stylesheets="@../../css/style.css" text="*Address" textFill="WHITE" />
      <TextField fx:id="customerAddress" layoutX="223.0" layoutY="175.0" prefHeight="25.0" prefWidth="260.0" />
      <Label layoutX="291.0" layoutY="404.0" styleClass="NEwindow-text" stylesheets="@../../css/style.css" text="required fields are marked with *" />
      <Button fx:id="createOrEditCustomer" layoutX="111.0" layoutY="399.0" mnemonicParsing="false" onAction="#createOrEditCustomer" styleClass="round-btn" stylesheets="@../../css/style.css" text="Edit" />
      <Button fx:id="cancelButton" layoutX="206.0" layoutY="399.0" mnemonicParsing="false" onAction="#cancel" styleClass="round-btn" stylesheets="@../../css/style.css" text="Cancel" />
      <TableView fx:id="postalCodeTV" layoutX="114.0" layoutY="215.0" prefHeight="170.0" prefWidth="373.0">
        <columns>
          <TableColumn fx:id="postalCode" prefWidth="123.0" text="Postal Code" />
          <TableColumn fx:id="city" prefWidth="249.0" text="City" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
