<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="650.0" prefWidth="950.0" stylesheets="@../../css/style.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.ManageCustomerController">
   <children>
      <TableView fx:id="customersTV" layoutX="100.0" layoutY="148.0" prefHeight="388.0" prefWidth="756.0">
        <columns>
          <TableColumn fx:id="customerAddress" prefWidth="179.0" text="Customer address" />
          <TableColumn fx:id="customerName" prefWidth="176.0" text="Customer" />
            <TableColumn fx:id="customerEmail" prefWidth="174.0" text="Customer email" />
            <TableColumn fx:id="projectName" prefWidth="220.0" text="Project" />
        </columns>
      </TableView>
      <Label fx:id="windowTitle" layoutX="100.0" layoutY="22.0" styleClass="window-title" text="Manage Customers" />
      <Label fx:id="usernameLabel" layoutX="717.0" layoutY="33.0" prefHeight="18.0" prefWidth="60.0" text=" " />
      <Button layoutX="813.0" layoutY="28.0" mnemonicParsing="false" onAction="#logOut" styleClass="round-btn" text="Log Out" />
      <Button layoutX="753.0" layoutY="565.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="103.0" styleClass="square-btn" text="Project Info" />
      <TextField fx:id="searchCustomer" layoutX="706.0" layoutY="106.0" promptText="search by address" />
      <Button layoutX="100.0" layoutY="580.0" mnemonicParsing="false" onAction="#createCustomer" prefHeight="30.0" prefWidth="97.0" styleClass="round-btn" text="New Customer" />
      <Button layoutX="245.0" layoutY="580.0" mnemonicParsing="false" onAction="#editCustomer" prefHeight="30.0" prefWidth="97.0" styleClass="round-btn" text="Edit Customer" />
   </children>
</AnchorPane>
